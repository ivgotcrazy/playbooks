application.name=sc

#kafka服务器集群地址
bootstrap.servers={{ kafka_group }}
#kafka group id，如果消费端出现topic异常，并且去服务器端检查topic是正常的，建议修改groupId的值
group.id=sc_group3
#zookeeper服务器集群地址
zookeeper.servers={{ zk_group }}
#zookeeper连接超时时间，单位：ms
zk.connection.timeout=5000
#zookeeper会话超时时间，单位：ms
zk.session.timeout=5000
#ice addr
ice.addr={{ ice_locator }}
#topic 分区数
topic.partions=3
#topic 副本数量
topic.replication=2
#当前sc 节点的topic名称
topic.sc.name = sc_instance86
#每一个SC集群都有一个所在的topic组
topic.sc.group.name = sc_group_20170906
#SC集群的topic组，用于与ms通信
topic.sc.group1.name = sc_ms_0906
#SC销毁流后通知媒体节点的topic
broadcast.topic=broadcast_topic
#consumer client id，同一个groupid的clientid不能一样
consumer.client.id=client86

#消息协议版本(kafka消息的读写版本号）
message.protocol.version = 1


#返回规则实例规则最大个数
max.instance.size = 5

#路径选择返回列表长度
routerStatus.return.size =5

#分布式锁配置
lock.root.path = /sclock
#getSuperiorStreamServer接口锁的超时时间,单位：s
lock.getSuperiorStreamServer.timeout=5
#service register parent node
service.parent.node=/fsp/sc


##########################################测试模拟异常情况使用###################################################

#路径选择是否可以直接连源节点开关，用于测试使用，0表示媒体服务可以经过2跳，也可以经过3跳，非0表示不能直连源节点，正式上线后该值只能为0
router.straight.switch=0
