application.name=ms

#kafka服务器集群地址
bootstrap.servers={{ kafka_group }}
#zookeeper服务器集群地址
zookeeper.servers={{ zk_group }}
#zookeeper连接超时时间，单位：ms
zk.connection.timeout=5000
#zookeeper会话超时时间，单位：ms
zk.session.timeout=5000
#zk acl 配置属性
zk.acl.name=ma
zk.acl.password=123456
#ice addr
ice.addr={{ ice_locator }}
#ms topic
topic.ms=ms_topic
group.id=ms_group1
#topic 分区数
topic.partions=1
#topic 副本数量
topic.replication=1
#每一个sC集群都有一个所在的topic组
topic.sc.group1.name = sc_ms_0906
#ms每隔多久下发一次统计网络状况任务,单位为：min
poll.time=10
#ms主节点目录
master.path=/fsp/master/ms
#各服务的zk注册目录
node.ma.parent=/fsp/ma
service.parent.node=/fsp/ms
access.path=/fsp/access
gc.path=/fsp/gc
rule.path=/fsp/rule
sc.path=/fsp/sc
sp.path=/fsp/sp
cp.path=/fsp/cp
ice.path=/fsp/ice
ss.path=/fsp/ss
gs.path=/fsp/gs

#cpu使用率过载值
cpu.overload = 1.0
#内存使用率过载值
mem.overload = 1.0
#带宽使用率过载值
bindwidth.overload = 1.0
